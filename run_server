#!/bin/bash

DIR=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )
mkdir -p processing 2>&1 >/dev/null

$DIR/kill_server
redis-server --daemonize yes --pidfile /tmp/Nredis.pid --logfile /tmp/Nredis.log
rqworker --pid /tmp/Nrqworker1.pid &
rqworker --pid /tmp/Nrqworker2.pid &
rqworker --pid /tmp/Nrqworker3.pid &
rqworker --pid /tmp/Nrqworker4.pid &
rqworker --pid /tmp/Nrqworker5.pid &
rqworker --pid /tmp/Nrqworker6.pid &
gunicorn --pid /tmp/Ngunicorn.pid --access-logfile /tmp/Ngunicorn.log --daemon -w 6 --graceful-timeout 1200 --timeout 1200 -b 0.0.0.0:5001 MasterApp:app
